									<xsd:annotation>
										<xsd:documentation>Nazwy spółek i okresy zawieszenia</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<!--L.2. INNE INFORMACJE-->
								<xsd:element name="P_324" type="tns:TTekstowy" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Inne informacje</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
