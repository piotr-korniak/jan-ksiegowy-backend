<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:czbr="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITBR/" xmlns:czd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITD/" xmlns:czf="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITF/" xmlns:czip="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITIP/" xmlns:czkw="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITKW/" xmlns:czm="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITM/" xmlns:czmit="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITMIT/" xmlns:cznz="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITNZI/" xmlns:czpd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPD/" xmlns:czpm="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPM/" xmlns:czww="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITWW/" xmlns:cz8o="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8O/" xmlns:cz8s="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8S/" xmlns:cz8sp="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8SP/" xmlns:etd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/DefinicjeTypy/" xmlns:kk="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2023/09/06/eD/KodyKrajow/" xmlns:kus="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/" xmlns:ssr="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/SSER/" xmlns:tns="http://crd.gov.pl/wzor/2024/02/08/13272/" xmlns:zzu="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/ORDZU/" targetNamespace="http://crd.gov.pl/wzor/2024/02/08/13272/" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="pl">
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITM/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITM/CIT_M(2)_Z_v1-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPD/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPD/CIT_PD(2)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/SSER/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/SSER/SSE-R(5)_Z_v11-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8O/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8O/CIT-8_O(19)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITBR/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITBR/CIT_BR(10)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITD/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITD/CIT-D(8)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITMIT/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITMIT/CIT_MIT(3)_Z_v7-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITNZI/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITNZI/CIT_NZI(2)_Z_v8-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8SP/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8SP/CIT_8SP(6)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8S/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CIT8S/CIT_8S(6)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITWW/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITWW/CIT_WW(3)_Z_v6-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPM/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITPM/CIT_PM(1)_Z_v7-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITIP/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITIP/CIT_IP(5)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITKW/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/02/05/eD/CITKW/CIT_KW(4)_Z_v2-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITF/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2024/01/12/eD/CITF/CIT_F(1)_Z_v1-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/ORDZU/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/ORDZU/ORD-ZU(3)_v10-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2023/09/06/eD/KodyKrajow/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2023/09/06/eD/KodyKrajow/KodyKrajow_v13-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/DefinicjeTypy/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/09/13/eD/DefinicjeTypy/StrukturyDanych_v12-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/KodyUrzedowSkarbowych_v8-0E.xsd"/>
	<!--Podstawa prawna: Art. 27 ust. 1 ustawy z dnia 15 lutego 1992 r. o podatku dochodowym od osób prawnych (Dz. U. z 2022 r. poz. 2587, z późn. zm.), zwanej dalej „ustawą”.-->
	<!-- -->
	<!--Składający: Podatnicy wymienieni w art. 1 ustawy, w zakresie dochodów określonych w art. 3 ust. 1 (mający siedzibę lub zarząd na terytorium RP) oraz w art. 3 ust. 2 (niemający siedziby lub zarządu na terytorium RP) ustawy, z wyjątkiem: 
- podatników uzyskujących przychody wyłącznie z tytułów wymienionych w art. 2 ust. 1 ustawy,
- podatników zwolnionych od składania zeznania - art. 27 ust. 1 ustawy,
- podatników niemających siedziby na terytorium RP, uzyskujących przychody, o których mowa w art. 21 i art. 22 ustawy wyłącznie od płatników wymienionych w art. 26 ust. 1 ustawy oraz podatnicy wymienieni w art. 7aa ustawy (składający informację CIT/KW).-->
	<!-- -->
	<!--Termin składania: Do końca trzeciego miesiąca następnego roku (art. 27 ust. 1 ustawy).-->
	<!-- -->
	<!--Miejsce składania: Urząd skarbowy.-->
	<!-- -->
	<!--Rokiem podatkowym jest rok kalendarzowy lub w przypadku, o którym mowa w art. 8 ust. 1 ustawy, okres kolejnych dwunastu miesięcy, z uwzględnieniem art. 8 ust. 2, 2a, 3, 6 i 6a ustawy. W przypadku podatników, o których mowa w art. 7aa ust. 8 ustawy, w poz. 4 i 5 należy wpisać dzień przekształcenia, który jest pierwszym dniem roku podatkowego podatnika.-->
	<!-- -->
	<!--Ilekroć jest mowa o urzędzie skarbowym, w tym urzędzie skarbowym, do którego jest adresowane zeznanie - oznacza to urząd skarbowy, przy pomocy którego właściwy dla podatnika naczelnik urzędu skarbowego wykonuje swoje zadania.-->
	<xsd:complexType name="TNaglowek">
		<xsd:annotation>
			<xsd:documentation>Nagłówek deklaracji</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KodFormularza">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TKodFormularza">
							<xsd:attribute name="kodSystemowy" type="xsd:string" use="required" fixed="CIT-8 (33)"/>
							<xsd:attribute name="kodPodatku" type="xsd:string" use="required" fixed="CIT"/>
							<xsd:attribute name="rodzajZobowiazania" type="xsd:token" use="required" fixed="Z"/>
							<xsd:attribute name="wersjaSchemy" type="xsd:string" use="required" fixed="2-0E"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="WariantFormularza">
				<xsd:simpleType>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="33"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CelZlozenia">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="etd:TCelZlozenia">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_7"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OkresOd">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TLDataP">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_4"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OkresDo">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TLDataP">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_5"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="KodUrzedu" type="kus:TKodUS"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="TKodFormularza">
		<xsd:annotation>
			<xsd:documentation>Symbol wzoru formularza</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CIT-8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TTekstowy">
		<xsd:annotation>
			<xsd:documentation>Typ tekstowy do 2000 znaków</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="etd:TTekstowy">
			<xsd:maxLength value="2000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TLDataP">
		<xsd:annotation>
			<xsd:documentation>Ograniczenie dla daty początku okresu</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="etd:TData">
			<xsd:minInclusive value="2022-01-01"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="TAdres1">
		<xsd:annotation>
			<xsd:documentation>Dane określające adres</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:sequence>
				<xsd:element name="AdresPol" type="tns:TAdresPolski1"/>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="AdresZagr" type="tns:TAdresZagraniczny"/>
			</xsd:sequence>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="TAdresPolski1">
		<xsd:annotation>
			<xsd:documentation>Informacje opisujące adres polski</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KodKraju">
				<xsd:annotation>
					<xsd:documentation>Kraj</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="kk:TKodKraju">
						<xsd:enumeration value="PL"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Wojewodztwo" type="etd:TJednAdmin">
				<xsd:annotation>
					<xsd:documentation>Województwo</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Powiat" type="etd:TJednAdmin">
				<xsd:annotation>
					<xsd:documentation>Powiat</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Gmina" type="etd:TJednAdmin">
				<xsd:annotation>
					<xsd:documentation>Gmina</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Ulica" type="etd:TUlica" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nazwa ulicy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NrDomu" type="etd:TNrBudynku">
				<xsd:annotation>
					<xsd:documentation>Numer budynku</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NrLokalu" type="etd:TNrLokalu" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numer lokalu</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Miejscowosc" type="etd:TMiejscowosc">
				<xsd:annotation>
					<xsd:documentation>Nazwa miejscowości</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KodPocztowy" type="etd:TKodPocztowy">
				<xsd:annotation>
					<xsd:documentation>Kod pocztowy</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TAdresZagraniczny">
		<xsd:annotation>
			<xsd:documentation>Informacje opisujące adres zagraniczny</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KodKraju">
				<xsd:annotation>
					<xsd:documentation>Kod Kraju [Country Code]</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="kk:TKodKraju">
						<xsd:pattern value="P[A-KM-Z]"/>
						<xsd:pattern value="[A-OQ-Z][A-Z]"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="KodPocztowy" type="etd:TKodPocztowy" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Kod pocztowy [Postal code]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Miejscowosc" type="etd:TMiejscowosc">
				<xsd:annotation>
					<xsd:documentation>Nazwa miejscowości [City]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Ulica" type="etd:TUlica" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Nazwa ulicy [Street]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NrDomu" type="etd:TNrBudynku" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numer budynku [Building number]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NrLokalu" type="etd:TNrLokalu" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Numer lokalu [Flat number]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TOsobaNiefizyczna2">
		<xsd:annotation>
			<xsd:documentation>Podstawowy zestaw danych o osobie niefizycznej</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OsobaNiefizyczna" type="etd:TIdentyfikatorOsobyNiefizycznej1"/>
			<xsd:element name="AdresSiedziby">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="tns:TAdres1">
							<xsd:attribute name="rodzajAdresu" type="xsd:string" use="required" fixed="RAD"/>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Deklaracja">
		<xsd:annotation>
			<xsd:documentation>ZEZNANIE O WYSOKOŚCI OSIĄGNIĘTEGO DOCHODU (PONIESIONEJ STRATY) I NALEŻNEGO PODATKU DOCHODOWEGO OD OSÓB PRAWNYCH</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Naglowek" type="tns:TNaglowek">
					<xsd:annotation>
						<xsd:documentation>Nagłówek deklaracji</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Podmiot1">
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="tns:TOsobaNiefizyczna2">
								<xsd:attribute name="rola" type="xsd:string" use="required" fixed="Podatnik"/>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PozycjeSzczegolowe">
					<xsd:annotation>
						<xsd:documentation>Dane dodatkowe, informacja o załącznikach, ustalenie dochodu / straty, ustalenie podstawy opodatkowania /straty, obliczenie należnego podatku, zaliczki miesięczne (art. 25 ust. 1, 1a oraz 6-6b ustawy) lub zaliczki kwartalne (art. 25 ust. 1b i 1c ustawy) oraz podatek, o którym mowa w art. 24b ustawy, różnica pomiędzy podatkiem należnym a należnymi zaliczkami, podatek do zapłaty/nadpłata, odrębne zobowiązanie podatkowe, inne zobowiązania podatkowe, informacje dodatkowe, informacje o okresach zawieszenia</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="P_8" type="etd:TWybor1_2" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Rodzaj korekty. Wartość: 1 - korekta zeznania, o której mowa w art. 81 Ordynacji podatkowej, 2 - korekta zeznania, o której mowa w art. 81b § 1a Ordynacji podatkowej</xsd:documentation>
								</xsd:annotation>
								<!--Ilekroć jest mowa o Ordynacji podatkowej, oznacza to ustawę z dnia 29 sierpnia 1997 r. - Ordynacja podatkowa (Dz. U. z 2023 r. poz. 2383).-->
								<!-- -->
								<!--W przypadku zaznaczenia w poz. 8 kwadratu nr 2 korektę składa się wraz z uzasadnieniem przyczyn korekty.-->
							</xsd:element>
							<xsd:element name="P_19" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik wybrał metodę ustalania daty powstawania przychodów na podstawie art. 12 ust. 3g ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:sequence minOccurs="0">
								<xsd:element name="P_20" type="etd:TWybor1">
									<xsd:annotation>
										<xsd:documentation>Podatnik uwzględnia odsetki w kosztach uzyskania przychodów zgodnie z art. 15c ustawy, w brzmieniu obowiązującym do dnia 31 grudnia 2017 r., w roku podatkowym wskazanym w poz. 4 i 5. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_21" type="etd:TKwota2Nieujemna">
									<xsd:annotation>
										<xsd:documentation>Odsetki wyłączone z kosztów uzyskania przychodów (wypełnia się w przypadku zaznaczenia poz. 20)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
							<xsd:element name="P_22" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik jest przedsiębiorstwem finansowym, o którym mowa w art. 15c ust. 16 ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_23" type="etd:TKwota2Nieujemna" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Koszty finansowania dłużnego wyłączone z kosztów uzyskania przychodów zgodnie z art. 15c ustawy, w roku podatkowym wskazanym w poz. 4 i 5</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_24" type="etd:TKwota2Nieujemna" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Koszty finansowania dłużnego wyłączone z kosztów uzyskania przychodów zgodnie z art. 16 ust. 1 pkt 13f ustawy</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_25" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik w roku podatkowym wskazanym w poz. 4 i 5 dokonuje obniżenia dochodu o stratę poniesioną przed utworzeniem podatkowej grupy kapitałowej,
na podstawie art. 7 ust. 6a ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
								<!--Wypełnia się w przypadku upływu obowiązywania umowy lub utraty przez podatkową grupę kapitałową statusu podatnika. Dotyczy strat spełniających warunki określone w art. 7 ust. 6a ustawy i w granicach określonych w przepisie art. 69b ustawy z dnia 29 października 2021 r. o zmianie ustawy o podatku dochodowym od osób fizycznych, ustawy o podatku dochodowym od osób prawnych oraz niektórych innych ustaw (Dz. U. poz. 2105, z późn. zm.).-->
							</xsd:element>
							<xsd:element name="P_26" type="etd:TWybor1_3">
								<xsd:annotation>
									<xsd:documentation>Podatnik korzysta ze zwolnienia lub utracił prawo do zwolnienia, o którym mowa w art. 25 ust. 11-16 ustawy, w roku podatkowym wskazanym w poz. 4 i 5. Wybór: 1 - tak, 2 - utracił prawo do zwolnienia w związku z art. 25 ust. 14 ustawy, 3 - nie</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_27" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik występuje o zwrot niewykorzystanego odliczenia na podstawie art. 18da ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:choice minOccurs="0">
								<xsd:element name="P_28" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał kwartalny sposób wpłacania zaliczek na podstawie art. 25 ust. 1b ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_29" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał wpłacanie zaliczek w formie uproszczonej na podstawie art. 25 ust. 6-6b ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
							<xsd:choice minOccurs="0">
								<xsd:element name="P_30" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał metodę ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_31" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik rezygnuje ze stosowania metody ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
							<xsd:element name="P_32" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podmiot powstały w wyniku podziału/połączenia oraz przejmujący całość / część innego podmiotu rezygnuje ze stosowania metody ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:sequence>
								<xsd:element name="P_33" type="etd:TWybor1_2">
									<xsd:annotation>
										<xsd:documentation>Podatnik przeniósł tymczasowo składniki majątku poza terytorium Rzeczypospolitej Polskiej. Wybór: 1 - tak, 2 - nie</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_34" type="etd:TKwota2Nieujemna" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Wartość rynkowa składników majątku tymczasowo przeniesionych w roku podatkowym, za który jest składane zeznanie, poza terytorium Rzeczypospolitej Polskiej (poz. 34 wypełniają podatnicy, którzy w poz. 33 wybrali wartość 1)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
							<xsd:sequence minOccurs="0">
								<xsd:element name="P_35" type="etd:TWybor1">
									<xsd:annotation>
										<xsd:documentation>Podatnik uwzględnia w kosztach uzyskania przychodów hipotetyczne odsetki od dopłat do spółki lub zysku przekazanego na kapitał rezerwowy lub zapasowy spółki, obliczone zgodnie z art. 15cb ust. 1 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_36">
									<xsd:annotation>
										<xsd:documentation>Łączne koszty hipotetycznych odsetek odliczone w roku podatkowym wskazanym w poz. 4 i 5</xsd:documentation>
									</xsd:annotation>
									<!--(wypełnia się w przypadku zaznaczenia kwadratu w poz. 35)-->
									<xsd:simpleType>
										<xsd:restriction base="etd:TKwota2Nieujemna">
											<xsd:minExclusive value="0"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
							</xsd:sequence>
							<!--C. INFORMACJA O ZAŁĄCZNIKACH-->
							<!-- -->
							<!--Należy podać liczbę dołączonych załączników.-->
							<xsd:sequence minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Informacja o załącznikach</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="P_37" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Informacja o załącznikach CIT/M</xsd:documentation>
									</xsd:annotation>
