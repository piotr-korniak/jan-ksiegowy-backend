										<xsd:documentation>Informacja o załącznikach CIT/WW</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_49" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Informacja o załącznikach CIT/PM</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="etd:TNaturalny">
											<xsd:minInclusive value="0"/>
											<xsd:maxInclusive value="99"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
								<xsd:element name="P_50" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
