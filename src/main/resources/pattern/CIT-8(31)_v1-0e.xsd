<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:czbr="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITBR/" xmlns:czd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITD/" xmlns:czip="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITIP/" xmlns:czkw="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITKW/" xmlns:czmit="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITMIT/" xmlns:cznz="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITNZI/" xmlns:czpm="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITPM/" xmlns:czst="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITSTZ/" xmlns:czww="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWW/" xmlns:czwz="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWZ/" xmlns:cz8o="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8O/" xmlns:cz8s="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8S/" xmlns:cz8sp="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8SP/" xmlns:etd="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/DefinicjeTypy/" xmlns:kus="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/" xmlns:ssr="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/SSER/" xmlns:tns="http://crd.gov.pl/wzor/2022/03/23/11433/" xmlns:zzu="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/14/eD/ORDZU/" targetNamespace="http://crd.gov.pl/wzor/2022/03/23/11433/" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="pl">
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITSTZ/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITSTZ/CIT-ST(8)_Z_v1-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/SSER/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/SSER/SSE-R(5)_Z_v8-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8O/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8O/CIT-8_O(17)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITBR/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITBR/CIT_BR(8)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITD/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITD/CIT-D(7)_Z_v4-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITMIT/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITMIT/CIT_MIT(3)_Z_v4-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITNZI/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITNZI/CIT_NZI(2)_Z_v5-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8SP/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8SP/CIT_8SP(5)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8S/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CIT8S/CIT_8S(5)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWW/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWW/CIT_WW(3)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITPM/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITPM/CIT_PM(1)_Z_v4-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITIP/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITIP/CIT_IP(3)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWZ/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITWZ/CIT_WZ(2)_Z_v3-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITKW/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/22/eD/CITKW/CIT_KW(2)_Z_v1-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/14/eD/ORDZU/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/03/14/eD/ORDZU/ORD-ZU(3)_v8-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/DefinicjeTypy/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/DefinicjeTypy/StrukturyDanych_v10-0E.xsd"/>
	<xsd:import namespace="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/" schemaLocation="http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2022/01/05/eD/KodyUrzedowSkarbowych/KodyUrzedowSkarbowych_v8-0E.xsd"/>
	<!--Rokiem podatkowym jest rok kalendarzowy lub w przypadku, o którym mowa w art. 8 ust. 1 ustawy, okres kolejnych dwunastu miesięcy, z uwzględnieniem art. 8 ust. 2, 2a, 3 i 6 ustawy.-->
	<!-- -->
	<!--Ilekroć jest mowa o urzędzie skarbowym, w tym urzędzie skarbowym, do którego jest adresowane zeznanie - oznacza to urząd skarbowy, przy pomocy którego właściwy dla podatnika naczelnik urzędu skarbowego wykonuje swoje zadania.-->
	<!-- -->
	<!--W wierszach ”Miesiące/Kwartały” liczby od 1 do 23 oznaczają kolejne miesiące roku podatkowego przy zaliczkach miesięcznych, a liczby 3, 6, 9, 12, 15, 18 i 21 odpowiednio kwartały roku podatkowego przy zaliczkach kwartalnych, w tym również u podatnika, którego rok podatkowy jest inny niż rok kalendarzowy.
		Podatnicy rozliczający zaliczki miesięczne wypełniają odpowiednio pozycje w zakresie poszczególnych miesięcy (od 1 do 23), a podatnicy rozliczający zaliczki kwartalne pozycje w zakresie zaliczek kwartalnych (3, 6, 9, 12, 15, 18 i 21).-->
	<xsd:complexType name="TNaglowek">
		<xsd:annotation>
			<xsd:documentation>Nagłówek deklaracji</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KodFormularza">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TKodFormularza">
							<xsd:attribute name="kodSystemowy" type="xsd:string" use="required" fixed="CIT-8 (31)"/>
							<xsd:attribute name="kodPodatku" type="xsd:string" use="required" fixed="CIT"/>
							<xsd:attribute name="rodzajZobowiazania" type="xsd:token" use="required" fixed="Z"/>
							<xsd:attribute name="wersjaSchemy" type="xsd:string" use="required" fixed="1-0E"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="WariantFormularza">
				<xsd:simpleType>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="31"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CelZlozenia">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="etd:TCelZlozenia">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_7"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OkresOd">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TLDataP">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_4"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OkresDo">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="tns:TLDataP">
							<xsd:attribute name="poz" type="xsd:string" use="required" fixed="P_5"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="KodUrzedu" type="kus:TKodUS"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="TKodFormularza">
		<xsd:annotation>
			<xsd:documentation>Symbol wzoru formularza</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CIT-8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TTekstowy">
		<xsd:annotation>
			<xsd:documentation>Typ tekstowy do 2000 znaków</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="etd:TTekstowy">
			<xsd:maxLength value="2000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TLDataP">
		<xsd:annotation>
			<xsd:documentation>Ograniczenie dla daty początku okresu</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="etd:TData">
			<xsd:minInclusive value="2021-01-01"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="Deklaracja">
		<xsd:annotation>
			<xsd:documentation>ZEZNANIE O WYSOKOŚCI OSIĄGNIĘTEGO DOCHODU (PONIESIONEJ STRATY) I NALEŻNEGO PODATKU DOCHODOWEGO OD OSÓB PRAWNYCH</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Naglowek" type="tns:TNaglowek">
					<xsd:annotation>
						<xsd:documentation>Nagłówek deklaracji</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Podmiot1">
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="etd:TOsobaNiefizyczna2">
								<xsd:attribute name="rola" type="xsd:string" use="required" fixed="Podatnik"/>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PozycjeSzczegolowe">
					<xsd:annotation>
						<xsd:documentation>Dane dodatkowe, informacja o załącznikach, ustalenie dochodu / straty, ustalenie podstawy opodatkowania /straty, obliczenie należnego podatku, zaliczki miesięczne (art. 25 ust. 1, 1a oraz 6-6b ustawy) lub zaliczki kwartalne (art. 25 ust. 1b i 1c ustawy) oraz podatek, o którym mowa w art. 24b ustawy, różnica pomiędzy podatkiem należnym a należnymi zaliczkami, kwota do zapłaty/nadpłata, odrębne zobowiązanie podatkowe, inne zobowiązania podatkowe, informacje dodatkowe, informacje o okresach zawieszenia</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="P_8" type="etd:TWybor1_2" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Rodzaj korekty. Wartość: 1 - korekta zeznania, o której mowa w art. 81 Ordynacji podatkowej, 2 - korekta zeznania, o której mowa w art. 81b § 1a Ordynacji podatkowej</xsd:documentation>
								</xsd:annotation>
								<!--Ilekroć jest mowa o Ordynacji podatkowej, oznacza to ustawę z dnia 29 sierpnia 1997 r. - Ordynacja podatkowa (Dz. U. z 2021 r. poz. 1540, z późn. zm.).-->
								<!-- -->
								<!--Ad. wartość 2 - Korekta składana w toku postępowania podatkowego w sprawie unikania opodatkowania; korektę składa się wraz z uzasadnieniem przyczyn korekty.-->
							</xsd:element>
							<xsd:element name="P_19" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik wybrał metodę ustalania daty powstawania przychodów na podstawie art. 12 ust. 3g ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:sequence minOccurs="0">
								<xsd:element name="P_20" type="etd:TWybor1">
									<xsd:annotation>
										<xsd:documentation>Podatnik uwzględnia odsetki w kosztach uzyskania przychodów zgodnie z art. 15c ustawy w brzmieniu obowiązującym do dnia 31 grudnia 2017 r., w roku podatkowym wskazanym w poz. 4 i 5. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_21" type="etd:TKwota2Nieujemna">
									<xsd:annotation>
										<xsd:documentation>Odsetki wyłączone z kosztów uzyskania przychodów (wypełnia się w przypadku zaznaczenia poz. 20)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
							<xsd:element name="P_22" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik jest przedsiębiorstwem finansowym, o którym mowa w art. 15c ust. 16 ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_23" type="etd:TKwota2Nieujemna" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Koszty finansowania dłużnego wyłączone z kosztów uzyskania przychodów zgodnie z art. 15c ustawy, w roku podatkowym wskazanym w poz. 4 i 5</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_24" type="etd:TKwota2Nieujemna" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Koszty wyłączone z kosztów uzyskania przychodów na podstawie art. 15e ustawy, w roku podatkowym wskazanym w poz. 4 i 5</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_25" type="etd:TWybor1_3">
								<xsd:annotation>
									<xsd:documentation>Podatnik korzysta ze zwolnienia lub utracił prawo do zwolnienia, o którym mowa w art. 25 ust. 11-16 ustawy, w roku podatkowym wskazanym w poz. 4 i 5. Wybór: 1 - tak, 2 - utracił prawo do zwolnienia w związku z art. 25 ust. 14 ustawy, 3 - nie</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_26" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik występuje o zwrot niewykorzystanego odliczenia na podstawie art. 18da ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="P_27" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podatnik dokonał korekty cen transferowych, o której mowa w art. 11e ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:choice minOccurs="0">
								<xsd:element name="P_28" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał kwartalny sposób wpłacania zaliczek na podstawie art. 25 ust. 1b ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_29" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał wpłacanie zaliczek w formie uproszczonej na podstawie art. 25 ust. 6-6b ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
									<!--Art. 25 ust. 6b ustawy obowiązuje od dnia 1 lipca 2020 r.-->
								</xsd:element>
							</xsd:choice>
							<xsd:choice minOccurs="0">
								<xsd:element name="P_30" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik wybrał metodę ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_31" type="etd:TWybor1" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Podatnik rezygnuje ze stosowania metody ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
							<xsd:element name="P_32" type="etd:TWybor1" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Podmiot powstały w wyniku podziału/połączenia oraz przejmujący całość / część innego podmiotu rezygnuje ze stosowania metody ustalania różnic kursowych na podstawie przepisów o rachunkowości, o której mowa w art. 9b ust. 1 pkt 2 ustawy. Wybór: 1 - tak</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:sequence>
								<xsd:element name="P_33" type="etd:TWybor1_2">
									<xsd:annotation>
										<xsd:documentation>Podatnik przeniósł tymczasowo składniki majątku poza terytorium Rzeczypospolitej Polskiej. Wybór: 1 - tak, 2 - nie</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_34" type="etd:TKwota2Nieujemna" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Wartość rynkowa składników majątku tymczasowo przeniesionych w roku podatkowym, za który jest składane zeznanie, poza terytorium Rzeczypospolitej Polskiej (poz. 34 wypełniają podatnicy, którzy w poz. 33 wybrali wartość 1)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
							<xsd:sequence minOccurs="0">
								<xsd:element name="P_35" type="etd:TWybor1">
									<xsd:annotation>
										<xsd:documentation>Podatnik uwzględnia w kosztach uzyskania przychodów hipotetyczne odsetki od dopłat do spółki lub zysku przekazanego na kapitał rezerwowy lub zapasowy spółki, obliczone zgodnie z art. 15cb ust. 1 ustawy. Wybór: 1 - tak</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_36">
									<xsd:annotation>
										<xsd:documentation>Łączne koszty hipotetycznych odsetek odliczone w roku podatkowym wskazanym w poz. 4 i 5</xsd:documentation>
									</xsd:annotation>
									<!--(wypełnia się w przypadku zaznaczenia kwadratu w poz. 35)-->
									<xsd:simpleType>
										<xsd:restriction base="etd:TKwota2Nieujemna">
											<xsd:minExclusive value="0"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
							</xsd:sequence>
							<xsd:sequence>
								<xsd:annotation>
									<xsd:documentation>Informacja o załącznikach</xsd:documentation>
								</xsd:annotation>
								<xsd:sequence>
									<!--Podatnicy posiadający zakłady (oddziały), położone na obszarze jednostki samorządu terytorialnego innej niż jednostka samorządu terytorialnego, na obszarze której podatnicy mają swoją siedzibę, w tym podatnicy prowadzący działalność poprzez położony na terytorium RP zakład zagraniczny, zatrudniający osoby na podstawie umowy o pracę - art. 28 ust. 1 i 2 ustawy oraz art. 10 ust. 1, 2 i 2a ustawy z dnia 13 listopada 2003 r. o dochodach jednostek samorządu terytorialnego (Dz. U. z 2021 r. poz. 1672, z późn. zm.). W przypadku zaznaczenia w poz. 37 kwadratu nr 1 należy podać liczbę załączników w poz. 38.-->
									<xsd:element name="P_37" type="etd:TWybor1_2">
										<xsd:annotation>
											<xsd:documentation>Podatnik jest zobowiązany do złożenia CIT-ST. Wybór: 1 - tak, 2 - nie</xsd:documentation>
										</xsd:annotation>
