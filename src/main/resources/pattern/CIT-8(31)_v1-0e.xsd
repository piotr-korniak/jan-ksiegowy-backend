									<xsd:annotation>
										<xsd:documentation>Wartość zobowiązań zwiększających podstawę opodatkowania, o których mowa w art. 18f ust. 1 pkt 2 ustawy</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="P_124" type="etd:TKwota2Nieujemna" minOccurs="0">
									<xsd:annotation>
